{
  "version": "0.2",
  "enableGlobDot": true,
  "files": [
    "**"
  ],
  "ignorePaths": [
    "/.git",
    "/.gitignore",
    ".vscode/extensions.json",
    "pnpm-lock.yaml"
  ],
  "useGitignore": true,
  "allowCompoundWords": true,
  "enableFiletypes": [
    "*"
  ],
  "includeRegExpList": [
    "[ -…è]"
  ],
  "words": [
    "nesw",
    "nofilter",
    "nvmrc",
    "zustand"
  ]
}
